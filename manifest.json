{
  "manifest_version": 2,
  "name": "YouTube Autoplay Disabler",
  "version": "0.1",
  "icons": {"128": "icon-small.png"},
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "browser_action": {
    "default_icon": "icon-small.png"
  }
}
